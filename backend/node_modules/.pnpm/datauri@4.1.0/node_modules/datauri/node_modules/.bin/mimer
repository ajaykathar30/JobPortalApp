#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/mimer@2.0.2/node_modules/mimer/bin/node_modules:/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/mimer@2.0.2/node_modules/mimer/node_modules:/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/mimer@2.0.2/node_modules:/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/mimer@2.0.2/node_modules/mimer/bin/node_modules:/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/mimer@2.0.2/node_modules/mimer/node_modules:/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/mimer@2.0.2/node_modules:/Users/ajaykathar/Documents/projects/ReactJS Projects/JobBoardApp/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mimer@2.0.2/node_modules/mimer/bin/mimer" "$@"
else
  exec node  "$basedir/../../../../../mimer@2.0.2/node_modules/mimer/bin/mimer" "$@"
fi
